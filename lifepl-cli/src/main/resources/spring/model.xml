<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="lifepl.model.slot.restriction.creators" scope="singleton" class="pl.edu.agh.integr10s.lifepl.model.world.restrictions.SlotRestrictionsFabric">
        <property name="restrictionCreators">
            <list>
                <bean class="pl.edu.agh.integr10s.lifepl.model.world.restrictions.creators.EnergyRestrictionCreator"/>
                <bean class="pl.edu.agh.integr10s.lifepl.model.world.restrictions.creators.SlotMembersCountRestrictionCreator"/>
            </list>
        </property>
    </bean>

</beans>